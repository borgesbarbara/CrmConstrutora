{
  "buildCommand": "cd frontend && npm install --force && CI=false DISABLE_ESLINT_PLUGIN=true npm run build-vercel",
  "outputDirectory": "frontend/build",
  "framework": null,
  "functions": {
    "backend/api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/index.js"
    }
  ],
  "regions": ["gru1"]
} 