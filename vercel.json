{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "framework": "create-react-app",
  "installCommand": "npm run install:all",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/test.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "regions": ["gru1"]
} 